# Development mode - enables manual IP entry
EXPO_PUBLIC_DEV_MODE=true

# Simulated node IP (Ubuntu machine running the simulator)
EXPO_PUBLIC_SIMULATED_NODE_IP=192.168.1.XXX

# Backend services (on Ubuntu)
EXPO_PUBLIC_COMMAND_CENTER_URL=http://192.168.1.XXX:8002
EXPO_PUBLIC_AUTH_API_BASE_URL=http://192.168.1.XXX:8007

# Node provisioning port (default 8080)
EXPO_PUBLIC_NODE_PORT=8080
